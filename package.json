{
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "probot run --webhook-path=/github/events .",
    "test": "jest --coverage --runInBand --forceExit",
    "posttest": "npm run lint",
    "lint": "eslint lib test && tslint lib test",
    "dev": "nodemon --exec npm start",
    "migrate": "sequelize db:migrate",
    "postinstall": "tsc"
  },
  "dependencies": {
    "@slack/client": "^3.14.2",
    "@slack/events-api": "^1.0.1",
    "axios": "^0.17.1",
    "body-parser": "^1.17.2",
    "express": "^4.16.2",
    "github": "^12.0.7",
    "html-to-mrkdwn": "github:github-slack/html-to-mrkdwn",
    "jsonwebtoken": "^8.1.0",
    "moment": "^2.18.1",
    "named-regexp": "^0.1.1",
    "pg": "^6.4.1",
    "probot": "github:probot/probot",
    "query-string": "^5.0.1",
    "redis": "^2.8.0",
    "sequelize": "^4.20.1",
    "sequelize-cli": "^3.1.0",
    "typescript": "^2.6.2"
  },
  "devDependencies": {
    "@types/jest": "^21.1.8",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "dotenv": "^4.0.0",
    "eslint": "^3.19.0 || ^4.5.0",
    "eslint-config-airbnb-base": "^11.3.2",
    "eslint-config-probot": ">=0.1.0",
    "eslint-import-resolver-typescript": "^1.0.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-typescript": "^0.8.1",
    "expect": "^1.20.2",
    "jest": "^21.2.1",
    "localtunnel": "^1.8.3",
    "nock": "^9.1.0",
    "nodemon": "^1.11.0",
    "snappydoo": "github:github-slack/snappydoo",
    "supertest": "github:bkeepers/supertest#unknown-errors",
    "ts-jest": "^21.2.4",
    "tslint": "^5.8.0",
    "typescript-eslint-parser": "^9.0.1"
  },
  "engines": {
    "node": "8.x.x",
    "npm": "5.x.x"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFiles": [
      "./test/setup"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverageFrom": [
      "lib/**/*.{js,ts}"
    ],
    "mapCoverage": true,
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$"
  },
  "main": "dist/index.js",
  "snappydoo": {
    "in": "test/slack/renderer",
    "out": "test/slack/renderer/renders",
    "exclude": [
      "AbstractIssue",
      "Message"
    ]
  }
}
